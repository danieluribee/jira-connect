<html>
<head>
</head>
<body style="margin: 0; background: #fff;">
    #if ($isLocal)
    <iframe
            id="jira-report"
            style="width: 100%; height: 100vh; border: none;"
            src="http://localhost:3000/standup-report?projectId=$projectId&clientKey=$clientKey&jwt=$jwt&server=true"
            frameborder="0">
    #elseif ($isStage)
    <iframe
            id="jira-report"
            style="width: 100%; height: 100vh; border: none;"
            src="https://standup-bot-stage.herokuapp.com/standup-report?projectId=$projectId&clientKey=$clientKey&jwt=$jwt&server=true"
            frameborder="0">
    #else
    <iframe
            id="jira-report"
            style="width: 100%; height: 100vh; border: none;"
            src="https://standbot.softwaredevtools.com/standup-report?projectId=$projectId&clientKey=$clientKey&jwt=$jwt&server=true"
            frameborder="0">
    #end
</iframe>
</body>
</html>
